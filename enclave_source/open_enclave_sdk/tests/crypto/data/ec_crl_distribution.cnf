# OpenSSL configuration for test CRL generation
#
####################################################################
[ req ]

default_bits            = 1024
default_md              = sha1
default_keyfile         = privkey.pem
req_extensions  = v3_req
x509_extensions = usr_cert
distinguished_name = req_distinguished_name

[req_distinguished_name]

CN = Intel SGX PCK Processor CA
O = Intel Corporation
L = Santa Clara
ST = CA
C = US



[ v3_req ]


authorityKeyIdentifier=DER:30168014E5BB528F80F9E333AE19ACFA63467811F361BBA4
crlDistributionPoints=crldp1_section


subjectKeyIdentifier=DER:0414CE29E95EFFE19789E46D483BB1F2DEC63BA4E51F

keyUsage=critical,digitalSignature, nonRepudiation

basicConstraints=critical,CA:FALSE


1.2.840.113741.1.13.1=DER:

[crldp1_section]

fullname=URI:https://certificates.trustedservices.intel.com/IntelSGXPCKProcessor.crl
