enclave {
    trusted {
        // Signed size should generate a warning.
        public void signed_size(
            [in, size=size] void* in,
            int size);

        // Unsigned size should not generate a warning.
        public void unsigned_size(
            [in, size=size] void* in,
            size_t size);
    };
};
